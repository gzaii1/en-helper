{
    "verbose": false,
    "debug": false,
    "exec": "ts-node -r ./src/index.ts",
    "ignore": [
      "mochawesome-report",
      "node_modules",
      "./test",
      "**/*.d.ts",
      "*.test.ts",
      "*.spec.ts",
      "fixtures/*",
      "test/**/*",
      "docs/*"
    ],
    "watch": ["src"],
    "ext": "ts tsx",
    "inspect": true
}